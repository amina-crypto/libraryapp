 <h2>Manage Users</h2>

<%= form_with url: search_users_path, method: :get do %>
  <%= label_tag :query, "Search by Name or Email" %>
  <%= text_field_tag :query, params[:query] %>
  <%= submit_tag "Search" %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li><%= user.email %> - Library User</li>
  <% end %>
</ul>

