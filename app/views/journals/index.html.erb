<p style="color: green"><%= notice %></p>

<% content_for :title, "Journals" %>

<h1>Journals</h1>

<div id="journals">
  <% @journals.each do |journal| %>
    <%= render journal %>
    <p>
      <%= link_to "Show this journal", journal %>
    </p>
  <% end %>
</div>

<%= link_to "New journal", new_journal_path %>
